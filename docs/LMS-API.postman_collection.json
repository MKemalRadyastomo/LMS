              "listen": "test",
              "script": {
                "exec": [
                  "const responseJson = pm.response.json();\r",
                  "pm.environment.set('authToken', responseJson.token);\r",
                  "pm.environment.set('userId', responseJson.user_id);\r",
                  "console.log('Authentication token saved to environment');"
                ],
                "type": "text/javascript",
                "packages": {}
              }
            }
