        {
          "name": "Register",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"email\": \"newuser@example.com\",\n    \"password\": \"Password123!\",\n    \"full_name\": \"New User\",\n    \"role\" : \"siswa\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/v1/auth/register",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "v1",
                "auth",
                "register"
              ]
            },
            "description": "Register a new user account (Requires Admin privileges)"
          },
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "const randomString = (length) => {",
                  "    let result = '';",
                  "    const characters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0987654321';",
                  "    const charactersLength = characters.length;",
                  "    for (let i = 0; i < length; i++) {",
                  "        result += characters.charAt(Math.floor(Math.random() * charactersLength));",
                  "    }",
                  "    return result;",
                  "};",
                  "",
                  "const randomEmail = `user_${randomString(8)}@example.com`;",
                  "const randomUsername = `user_${randomString(6)}`;",
                  "const roles = [\"siswa\", \"guru\", \"admin\"];",
                  "const randomRole = roles[Math.floor(Math.random() * roles.length)];",
                  "",
                  "const requestBody = JSON.parse(pm.request.body.raw);",
                  "requestBody.email = randomEmail;",
                  "requestBody.full_name = randomUsername; // Assuming full_name can be used as username",
                  "requestBody.role = randomRole;",
                  "",
                  "pm.request.body.raw = JSON.stringify(requestBody, null, 2);",
                  "",
                  "console.log(`Generated Email: ${randomEmail}`);",
                  "console.log(`Generated Username: ${randomUsername}`);",
                  "console.log(`Generated Role: ${randomRole}`);"
                ]
              }
            }
          ],
          "response": []
        },
